{
    "name": "Clean Light Inspired",
    "type": "light",
    "colors": {
        "editor.background": "#ffffff",
        "editor.foreground": "#1c1c1d",
        "editor.lineHighlightBackground": "#f8f8f8",
        "editor.selectionBackground": "#cce5ff",
        "editorCursor.foreground": "#1d1c1c",
        "editorWhitespace.foreground": "#ffffff",
        "activityBar.background": "#212328",
        "activityBar.foreground": "#ffffff",
        "sideBar.background": "#f4f4f4",
        "sideBar.foreground": "#1d1c1c",
        "sideBar.border": "#f4f4f4",
        "statusBar.background": "#5b9c00",
        "statusBar.foreground": "#ffffff",
        "titleBar.activeBackground": "#ffffff",
        "titleBar.activeForeground": "#1d1c1d",
        "titleBar.inactiveForeground": "#a8a8a8",

        "list.inactiveSelectionBackground": "#5b9c00",
        "list.inactiveSelectionForeground": "#ffffff",
        "list.hoverBackground": "#e1e1e1",
        "list.highlightForeground": "#1d1c1d",
        "tab.activeBackground": "#ffffff",
        "tab.activeForeground": "#1d1c1d",
        "tab.inactiveBackground": "#f4f4f4",
        "tab.inactiveForeground": "#a8a8a8",
        "tab.activeModifiedBorder": "#1c1c1d",
        "tab.inactiveModifiedBorder": "#1c1c1d",
        "editorLineNumber.foreground": "#a8a8a8",
        "editorLineNumber.activeForeground": "#1d1c1d",
        "editorGutter.modifiedBackground": "#1c1c1d",
        "editorGutter.addedBackground": "#1c1c1d",
        "editorGutter.deletedBackground": "#5b9c00",

        "list.activeSelectionBackground": "#ffffff",
        "list.activeSelectionForeground": "#1c1c1d",
        "gitDecoration.modifiedResourceForeground": "#000000",
        "gitDecoration.stageModifiedResourceForeground": "#000000",

        "editorIndentGuide.background1": "#f0f0f0",
        "editorIndentGuide.activeBackground1": "#f0f0f0",

        "editorUnnecessaryCode.border": "#ff0000",
        "editorUnnecessaryCode.opacity": "#000000c0"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#9a9a9a"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#e01e5a"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#1d1c1c"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
				"fontStyle": "bold",
                "foreground": "#008cff"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                // "fontStyle": "italic",
                "foreground": "#5b9c00"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant"
            ],
            "settings": {
				"fontStyle": "bold",
                "foreground": "#e01e5a"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#1d1c1c"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#e01e5a"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#1d1c1c"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#4b1515"
            }
        },
        {
            "name": "HTML: Tags",
            "scope": [
                "meta.tag",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html"
            ],
            "settings": {
                "foreground": "#1c1c1d"
            }
        },
        {
            "name": "HTML: Tag Names",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#5b9c00"
            }
        },
        {
            "name": "HTML: Attribute Names",
            "scope": [
                "meta.tag entity.other.attribute-name",
                "entity.other.attribute-name.html"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#000000"
            }
        },
        {
            "name": "CSS: Selectors",
            "scope": [
                "meta.selector",
                "meta.selector entity",
                "meta.selector entity punctuation",
                "entity.name.tag.css"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#1c1c1d"
            }
        },
        {
            "name": "CSS: Property Names",
            "scope": [
                "meta.property-name",
                "support.type.property-name"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#000000"
            }
        },
        {
            "name": "CSS: Property Values",
            "scope": [
                "meta.property-value",
                "meta.property-value constant.other",
                "support.constant.property-value"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#e01e5a"
            }
        },
        {
            "name": "CSS: Important Keyword",
            "scope": "keyword.other.important",
            "settings": {
                "fontStyle": "",
                "foreground": "#77ac66"
            }
        },
        {
            "name": "Markup: Changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#1d1c1d"
            }
        },
        {
            "name": "Markup: Deletion",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#77ac66"
            }
        },
        {
            "name": "Markup: Emphasis",
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
			"name": "Markup: Error",
            "scope": [
                "invalid",
                "markup.error",
                "source.error"
            ],
            "settings": {
                "foreground": "#ff0000"
            }
        },
        {
            "name": "Markup: Insertion",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#1c1c1d"
            }
        },
        {
            "name": "Markup: Link",
            "scope": "meta.link",
            "settings": {
                "foreground": "#e01e5a"
            }
        },
        {
            "name": "Markup: Heading",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#1c1c1d"
            }
        },
        {
            "name": "Markup: Strong",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Object Literal Keys",
            "scope": [
                "meta.object-literal.key",
                "meta.object-literal.key string"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#1c1c1d"
            }
        },
        {
            "name": "Unused Variable",
            "scope": [
                "variable.other.readwrite.unused",
                "meta.unused",
                "source.unused",
                "variable.other.unused"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#a0a0a0"
            }
        },
        {
            "name": "Braces",
            "scope": [
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end",
                "meta.brace.curly"
            ],
            "settings": {
                "foreground": "#1c1c1d"
            }
        }
    ]
}
